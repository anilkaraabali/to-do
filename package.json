    "i18next-browser-languagedetector": "^6.0.1",
    "node-sass": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.4",
    "react-i18next": "^11.8.10",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
